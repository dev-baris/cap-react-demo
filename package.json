    "@capacitor/share": "^5.0.2",
    "@capacitor/splash-screen": "^5.0.2",
    "@capacitor/status-bar": "^5.0.2",
    "@capacitor/text-zoom": "^5.0.2",
    "@capacitor/toast": "^5.0.2",
