{
  "name": "capacitor-react",
  "version": "0.0.1",
  "author": "Baris Aydin <dev.barisaydin@gmail.com>",
  "private": true,
  "description": "Capacitor React",
  "scripts": {
    "start": "vite",
    "build": "tsc && vite build",
    "fmt": "npm run prettier -- --write",
    "lint": "npm run prettier -- --check",
    "prettier": "prettier '{src,scripts,public}/**/*.{css,html,ts,tsx,mjs}'",
    "toggle-local": "node ./scripts/toggle-local.mjs",
    "update-all": "node ./scripts/update-all.mjs"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not ie <= 11",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
